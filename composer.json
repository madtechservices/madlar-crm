{
    "name": "tomatophp/tomato-crm",
    "type": "library",
    "description": "full accounts manager with API/Notifications/Contacts to manage your contacts and accounts",
    "keywords": [
        "php",
        "laravel",
        "template"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoCrm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoCrm\\TomatoCrmServiceProvider"
            ],
            "aliases": {
                "TomatoCrm": "TomatoPHP\\TomatoCrm\\Facades\\TomatoCrm"
            }
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.0",
        "tomatophp/tomato-admin": "^1.1",
        "tomatophp/tomato-category": "^1.1",
        "tomatophp/tomato-locations": "^1.1",
        "tomatophp/tomato-notifications": "^1.1",
        "tomatophp/tomato-translations": "^1.1",
        "spatie/macroable": "^2.0"
    }
}
